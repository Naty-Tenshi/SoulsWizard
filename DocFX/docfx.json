{
  "metadata": [
    {
      "src": [
        {
          "src": "../Unity/Assets/TecWolf/Scripts/",
          "files": [
            "/**.cs"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "site/**"
        ]
      }
    ],
    "dest": "site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appLogoPath": "../images/logo.png",
      "_appFaviconPath": "../images/favicon.png",
      "_appFooter": "<div id='google_translate_element'></div> <style>body { top: 0px !important;  } .goog-te-banner-frame.skiptranslate { display: none !important; } .navbar.navbar-inverse.ng-scope, .navbar-inverse .navbar-nav > li > a { background-color: #9c27b0 !important; } .navbar-brand { padding-top: 7.5px !important; padding-right: 7.5px !important; }</style> <script type='text/javascript'>function GoogleTranslate() { new google.translate.TranslateElement({ pageLanguage: 'en', layout: google.translate.TranslateElement.InlineLayout.SIMPLE }, 'google_translate_element'); }</script> <script type='text/javascript' src='//translate.google.com/translate_a/element.js?cb=GoogleTranslate'></script>"
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "material/material"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}